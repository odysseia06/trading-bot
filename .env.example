# Binance Trading Bot Configuration
# Copy this file to .env and fill in your values

# Binance API credentials
# Get production keys from: https://www.binance.com/en/my/settings/api-management
# Get testnet keys from: https://testnet.binance.vision/
BINANCE_API_KEY=your_api_key_here
BINANCE_SECRET_KEY=your_secret_key_here

# Environment: production or testnet
# Can also use: prod, mainnet, test, sandbox
BINANCE_ENVIRONMENT=testnet

# Logging level (optional)
# Options: trace, debug, info, warn, error
RUST_LOG=info
